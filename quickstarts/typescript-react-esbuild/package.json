{
  "name": "typescript-react-esbuild-quickstart",
  "type": "module",
  "scripts": {
    "build": "esbuild --bundle --outfile=build/bundle.js --sourcemap src/index.tsx",
    "build:min": "esbuild --bundle --minify --outfile=build/bundle.min.js --sourcemap src/index.tsx"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "esbuild": "^0.12.19"
  }
}
